@page
@model KitapYorum.RazorPage.Pages.Account.RegisterModel
@{
}
<div class="card">
    <div class="card-header">
        <h3>Register </h3>
    </div>
    <div class="card-body">
        <form method="post">
            <div class="mb-3">
                <label for="@Model.CreateUser.Email" class="form-label">Email address</label>
                <input asp-for="@Model.CreateUser.Email" class="form-control" placeholder="name@example.com">
            </div>
            <div class="mb-3">
                <label for="@Model.CreateUser.TcNo" class="form-label">TcNo </label>
                <input asp-for="@Model.CreateUser.TcNo" class="form-control" placeholder="12345678912">
            </div>
            <div class="mb-3">
                <label asp-for="@Model.CreateUser.Password" class="form-label">Password </label>
                <input asp-for="@Model.CreateUser.Password" class="form-control">
            </div>
            <div class="mb-3">
                <label asp-for="@Model.CreateUser.RePassword" class="form-label">Password </label>
                <input asp-for="@Model.CreateUser.RePassword" class="form-control">
            </div>
          

            <button class="btn btn-primary" type="submit">Kaydet</button>
        </form>
    </div>
</div>